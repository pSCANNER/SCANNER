<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:camel="http://camel.apache.org/schema/spring"      
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd    
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

    <bean id="OceansLogisticRegressionAggregateProcessor"
          class="edu.isi.misd.scanner.network.modules.master.processors.oceans.OceansLogisticRegressionAggregateProcessor"/>
                      
    <camel:routeContext id="OceansRouteContext">
                
        <camel:route>
            <camel:from uri="jetty:http://{{master.address}}:{{master.port}}/{{master.appDomain}}/{{master.appContext}}/oceans/lr?sessionSupport=false&amp;matchOnUriPrefix=true"/>          
            <camel:to uri="direct:OceansLogisticRegressionRoute"/>
        </camel:route>
        
    </camel:routeContext>
    
</beans>
