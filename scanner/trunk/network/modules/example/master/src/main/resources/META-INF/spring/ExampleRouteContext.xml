<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:camel="http://camel.apache.org/schema/spring"      
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd    
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">
                     
    <bean id="IterationAggregateProcessor"
          class="edu.isi.misd.scanner.network.modules.master.processors.example.IterationAggregateProcessor"/>  
         
    <camel:routeContext id="ExampleRouteContext">
       
        <camel:route>
            <camel:from uri="jetty:http://{{master.address}}:{{master.port}}/{{master.appDomain}}/{{master.appContext}}/example/echo?sessionSupport=false&amp;matchOnUriPrefix=true"/>          
            <camel:to uri="direct:DefaultRoute"/>
        </camel:route>
                         
        <camel:route>
            <camel:from uri="jetty:http://{{master.address}}:{{master.port}}/{{master.appDomain}}/{{master.appContext}}/example/iteration?sessionSupport=false&amp;matchOnUriPrefix=true"/>          
            <camel:to uri="direct:IterationRoute"/>
        </camel:route>
        
    </camel:routeContext>
    
</beans>
