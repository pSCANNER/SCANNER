<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:camel="http://camel.apache.org/schema/spring"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">
            
    <bean id="IterationProcessor"
          class="edu.isi.misd.scanner.network.modules.worker.processors.example.IterationProcessor"/>  
           
    <camel:routeContext id="ExampleRouteContext">
    
        <camel:route>
            <camel:from uri="jetty:http://{{worker.address}}:{{worker.port}}/{{worker.appDomain}}/{{worker.appContext}}/example/echo?sessionSupport=false&amp;matchOnUriPrefix=true"/>
            <camel:to uri="direct:DefaultRoute"/>                     
        </camel:route>

        <camel:route>
            <camel:from uri="jetty:http://{{worker.address}}:{{worker.port2}}/{{worker.appDomain}}/{{worker.appContext}}/example/echo?sessionSupport=false&amp;matchOnUriPrefix=true"/>
            <camel:to uri="direct:DefaultRoute"/>                     
        </camel:route>
        
        <camel:route>
            <camel:from uri="jetty:http://{{worker.address}}:{{worker.port3}}/{{worker.appDomain}}/{{worker.appContext}}/example/echo?sessionSupport=false&amp;matchOnUriPrefix=true"/>
            <camel:to uri="direct:DefaultRoute"/>                     
        </camel:route>
        
        <camel:route>
            <camel:from uri="jetty:http://{{worker.address}}:{{worker.port4}}/{{worker.appDomain}}/{{worker.appContext}}/example/echo?sessionSupport=false&amp;matchOnUriPrefix=true"/>
            <camel:to uri="direct:DefaultRoute"/>                     
        </camel:route>
                                                                                                                                                                                                                                                                
        <camel:route>
            <camel:from uri="jetty:http://{{worker.address}}:{{worker.port}}/{{worker.appDomain}}/{{worker.appContext}}/example/iteration?sessionSupport=false&amp;matchOnUriPrefix=true"/>
            <camel:to uri="direct:IterationRoute"/>                     
        </camel:route>

        <camel:route>
            <camel:from uri="jetty:http://{{worker.address}}:{{worker.port2}}/{{worker.appDomain}}/{{worker.appContext}}/example/iteration?sessionSupport=false&amp;matchOnUriPrefix=true"/>             
            <camel:to uri="direct:IterationRoute"/>                         
        </camel:route>        
                                                                                                                                                                                                                                                                                 
        <camel:route>
            <camel:from uri="jetty:http://{{worker.address}}:{{worker.port3}}/{{worker.appDomain}}/{{worker.appContext}}/example/iteration?sessionSupport=false&amp;matchOnUriPrefix=true"/>             
            <camel:to uri="direct:IterationRoute"/>                             
        </camel:route>
        
        <camel:route>
            <camel:from uri="jetty:http://{{worker.address}}:{{worker.port4}}/{{worker.appDomain}}/{{worker.appContext}}/example/iteration?sessionSupport=false&amp;matchOnUriPrefix=true"/>             
            <camel:to uri="direct:IterationRoute"/>                                    
        </camel:route>
        
    </camel:routeContext>
    
</beans>
