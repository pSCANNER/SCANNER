<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>scanner</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>edu.isi.misd.scanner.servlets.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Tagfiler</servlet-name>
    <servlet-class>edu.isi.misd.scanner.servlets.Tagfiler</servlet-class>
    <init-param>
    	<param-name>tagfilerURL</param-name>
    	<param-value>https://serbancentos.isi.edu/tagfiler</param-value>
    </init-param>
    <init-param>
    	<param-name>tagfilerUser</param-name>
    	<param-value>guest</param-value>
    </init-param>
    <init-param>
    	<param-name>tagfilerPassword</param-name>
    	<param-value>just4demo</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Tagfiler</servlet-name>
    <url-pattern>/tagfiler</url-pattern>
  </servlet-mapping>
</web-app>